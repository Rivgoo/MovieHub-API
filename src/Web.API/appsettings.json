{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FakeDataSeed": {
    "Enabled": false
  },
  "UseSwagger": true,
  "AllowedHosts": "*",
  "ApplyMigrations": true,
  "RequireEmailConfirmedToSignIn": true,
  "MaxFailedAccessAttempts": 6,
  "DefaultLockoutInMinutes": 10,
  "EnableAPIAuth": true,
  "PublicDataFolder": "PublicData",
  "ContentPosterPath": "content/posters",
  "ContentPosterMaxSizeInKilobytes": 5120,
  "ContentBannerPath": "content/banners",
  "ContentBannerMaxSizeInKilobytes": 5120,
  "ActorPhotoPath": "actors/photos",
  "ActorPhotoMaxSizeInKilobytes": 2048
}